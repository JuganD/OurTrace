{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.\\SQLEXPRESS;Database=OurTrace;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Storage": {
    "Providers": {
      "FileSystemProvider": {
        "Type": "FileSystem"
      }
    },
    "Stores": {
      "LocalFileStorage": {
        "ProviderName": "FileSystemProvider",
        "Parameters": {
          "Path": "LocalFileStorage"
        }
      }
    }
  }
}
